{
  "name": "makehex",
  "private": true,
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@rails/webpacker": "^4.0.0-rc.8",
    "actioncable": "^5.2.1",
    "babel-core": "^7.0.0-0",
    "eventemitter": "^0.3.3",
    "honeycomb-grid": "^1.3.6",
    "mini-css-extract-plugin": "^0.7.0",
    "svg-spritemap-webpack-plugin": "^3.3.4",
    "svg.js": "^2.6.6",
    "vue": "^2.5.17",
    "vue-loader": "^15.7.0",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.8.0",
    "expect.js": "^0.3.1",
    "jest": "^24.8.0",
    "jest-tap-reporter": "^1.9.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "vue-jest": "^3.0.4",
    "webpack-dev-server": "2.11.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/app/javascript/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 1,
        "functions": 1,
        "lines": 1,
        "statements": 1
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "reporters": [
      "jest-tap-reporter"
    ],
    "testMatch": [
      "<rootDir>/spec/javascript/**/*.spec.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  }
}
